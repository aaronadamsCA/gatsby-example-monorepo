{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "cross-env NODE_ENV=test eslint --cache .",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf .eslintcache tsconfig.tsbuildinfo"
  },
  "engines": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-preset-gatsby": "2.1.3",
    "core-js": "3.19.1",
    "cross-env": "7.0.3",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.27.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.4"
  }
}
